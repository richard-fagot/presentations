/******/ (function(modules) { // webpackBootstrap
/******/ 	// The module cache
/******/ 	var installedModules = {};

/******/ 	// The require function
/******/ 	function __webpack_require__(moduleId) {

/******/ 		// Check if module is in cache
/******/ 		if(installedModules[moduleId])
/******/ 			return installedModules[moduleId].exports;

/******/ 		// Create a new module (and put it into the cache)
/******/ 		var module = installedModules[moduleId] = {
/******/ 			exports: {},
/******/ 			id: moduleId,
/******/ 			loaded: false
/******/ 		};

/******/ 		// Execute the module function
/******/ 		modules[moduleId].call(module.exports, module, module.exports, __webpack_require__);

/******/ 		// Flag the module as loaded
/******/ 		module.loaded = true;

/******/ 		// Return the exports of the module
/******/ 		return module.exports;
/******/ 	}


/******/ 	// expose the modules object (__webpack_modules__)
/******/ 	__webpack_require__.m = modules;

/******/ 	// expose the module cache
/******/ 	__webpack_require__.c = installedModules;

/******/ 	// __webpack_public_path__
/******/ 	__webpack_require__.p = "";

/******/ 	// Load entry module and return exports
/******/ 	return __webpack_require__(0);
/******/ })
/************************************************************************/
/******/ ([
/* 0 */
/***/ function(module, exports, __webpack_require__) {

	"use strict";

	// This file requires everything and houses the global functions
	var ExperienceStore = __webpack_require__(1);
	var Environment = __webpack_require__(21);
	var Player = __webpack_require__(12);
	var Playlist = __webpack_require__(26);
	var Events = __webpack_require__(13);
	var Api = __webpack_require__(14);
	var AppDispatcher = __webpack_require__(2).Dispatcher;
	var dispatch = __webpack_require__(2).dispatch;
	var Buttons = __webpack_require__(40);
	var Helpers = __webpack_require__(23);
	var IrisStartNextSlate = __webpack_require__(28);
	var IrisEndNextSlate = __webpack_require__(31);
	var jQuery = __webpack_require__(24);
	var logger = __webpack_require__(22);
	var SideRail = __webpack_require__(42);
	var EventConstants = __webpack_require__(20);
	var RelatedRail = __webpack_require__(43);
	// GLOBAL HELPER TO INITIALIZE PLUGIN  

	window.initializeIrisPlugin = function (options) {
	  if (options.debug === true) {
	    logger.setLevel("DEBUG");
	  } else {
	    logger.setLevel("INFO");
	  }
	  logger.info("Library Version: [IRIS Adaptive: v" + Environment.VERSION_NUMBER + "]");

	  var eid = Helpers.generateRandomString(4);

	  // Adds default settings to options hash 
	  var irisOptions = validateSettings(options);
	  logger.debug("IRIS config: ", irisOptions);

	  var experience = ExperienceStore.initialize(eid, irisOptions);

	  function registerFunction(IrisFunctionName, newFunction) {
	    logger.debug("Registering new function: " + IrisFunctionName);
	    switch (IrisFunctionName) {
	      case "currentDuration":
	        experience.player.currentDuration = newFunction;
	        break;
	      case "currentTime":
	        experience.player.currentTime = newFunction;
	        break;
	      case "play":
	        experience.player.play = function (asset, player_id) {
	          var player_id = player_id ? player_id : "";
	          newFunction(asset, player_id);
	          Events.play(this.eid);
	        };
	        break;
	      case "loadVideo":
	        experience.player.loadVideo = newFunction;
	        break;
	      case "validateVideo":
	        experience.player.validateVideo = newFunction;
	        break;
	      case "attachToPlayer":
	        experience.player.attachToPlayer = newFunction;
	        break;
	      case "IrisStartNextSlate.populate":
	        IrisStartNextSlate.prototype.populate = function () {
	          var experience = ExperienceStore.getInfo(this.eid);
	          nextVideo = experience.playlist.getInfo()[experience.currentIndex + 1];
	          newFunction(nextVideo);
	        };
	        break;

	    }
	  }

	  function registerEvent(IrisEventName, newEvent) {
	    switch (IrisEventName) {
	      case "IRIS_PLAYING":
	        logger.debug("IRIS: registering event IRIS_PLAYING");
	        Events.playing = function () {};
	        break;
	      case "ENDED":
	        logger.debug("IRIS: registering event ENDED");
	        Events.ended = function () {};
	        break;
	    }
	  }

	  function emit(IrisEvent, data) {
	    dispatch({
	      "actionType": IrisEvent,
	      "data": data,
	      "experienceId": eid
	    });
	  }

	  function forceNextRequest() {
	    Api.make_next_request(eid);
	  }
	  function forceWatchRequest() {
	    Api.make_watch_request(eid);
	  }

	  function getPlaylist() {
	    return ExperienceStore.getPlaylist(eid);
	  }
	  function setPlaylist(arr) {
	    ExperienceStore.getInfo(eid).playlist.addToPlaylist(arr);
	    return arr;
	  }
	  function getCurrentIndex() {
	    return ExperienceStore.getCurrentIndex(eid);
	  }
	  function skipToIndex(index) {
	    return ExperienceStore.skipToIndex(eid, index);
	  }

	  function setAdState(state) {
	    ExperienceStore.getInfo(eid).player.setAdState(state);
	    return ExperienceStore.getInfo(eid).player.getAdState();
	  }

	  var addCallback = {
	    addToPlaylist: function addToPlaylist(callback) {
	      var oldFunction = Playlist.prototype.addToPlaylist;
	      Playlist.prototype.addToPlaylist = function () {
	        var playlist = this;
	        oldFunction();
	        callback(this.recs);
	      };
	    },
	    next: function next(callback) {
	      AppDispatcher.register(function (action) {
	        switch (action.actionType) {
	          case EventConstants.NEXT_SUCCESS:
	            callback(action.response.next);
	            break;
	        }
	      });
	    },
	    watch: function watch(callback) {
	      AppDispatcher.register(function (action) {
	        switch (action.actionType) {
	          case EventConstants.WATCH_SUCCESS:
	            callback(action.response.next);
	            break;
	        }
	      });
	    }
	  };

	  function validateSettings(options) {
	    var requiredSettings = ['number', 'base_url', 'client_token', 'platform', 'access_token', 'player_id'];

	    var player_elements = options.player_elements !== undefined ? options.player_elements : {};
	    var player_id = options.settings.player_id !== undefined ? options.settings.player_id : jQuery('[iris-enabled=true]').id;
	    var number = options.settings.number !== undefined ? options.settings.number : 5;
	    var video_player = player_elements.video_player;
	    var ssl = options.settings.ssl !== undefined ? options.settings.ssl : false;
	    var base_url = options.settings.base_url !== undefined ? options.settings.base_url : 'defaultbaseurl';
	    var client_token = options.settings.client_token !== undefined ? options.settings.client_token : jQuery('#' + player_id).attr('client-token');
	    var platform = options.settings.platform !== undefined ? options.settings.platform : jQuery('#' + player_id).attr('platform');
	    var access_token = options.settings.access_token !== undefined ? options.settings.access_token : "1d9f05c8b00daddfbffcf5afa8a0691bf6370c0cd9dfc8bc6fb38e13c4474dab";
	    var api_url = options.settings.api_url;
	    var platform_id = options.settings.platform_id !== undefined ? options.settings.platform_id : jQuery('#' + player_id).attr('platform-id');
	    var player_version = options.settings.player_version !== undefined ? options.settings.player_version : platform;

	    // UpNext configurations
	    var disable_mobile_upnext = options.settings.disable_mobile_upnext !== undefined ? options.settings.disable_mobile_upnext : true;
	    var up_next_min_vid_length = options.settings.up_next_min_vid_length !== undefined ? options.settings.up_next_min_vid_length : 23;

	    var start_up_next = options.settings.start_up_next !== undefined ? options.settings.start_up_next : false;
	    var start_up_next_text = options.settings.start_up_next_text !== undefined ? options.settings.start_up_next_text : "UP NEXT:";
	    var start_up_next_time = options.settings.start_up_next_time !== undefined ? options.settings.start_up_next_time : 5;
	    var start_up_next_length = options.settings.start_up_next_length !== undefined ? options.settings.start_up_next_length : 7;

	    var end_up_next = options.settings.end_up_next !== undefined ? options.settings.end_up_next : false;
	    var end_up_next_text = options.settings.end_up_next_text !== undefined ? options.settings.end_up_next_text : "UP NEXT:";
	    var end_up_next_time = options.settings.end_up_next_time !== undefined ? options.settings.end_up_next_time : 5;
	    var end_up_next_length = options.settings.end_up_next_length !== undefined ? options.settings.end_up_next_length : 7;

	    // Initialize iris_buttons as empty hash if not sent in my client
	    var iris_buttons = options.iris_buttons !== undefined ? options.iris_buttons : {};
	    var thumbs_up = iris_buttons.thumbs_up !== undefined ? iris_buttons.thumbs_up : true;
	    var thumbs_down = iris_buttons.thumbs_down !== undefined ? iris_buttons.thumbs_down : true;
	    var skip_forward = iris_buttons.skip_forward !== undefined ? iris_buttons.skip_forward : true;
	    var skip_back = iris_buttons.skip_back !== undefined ? iris_buttons.skip_back : true;
	    var skip_on_thumbs_down = iris_buttons.skip_on_thumbs_down !== undefined ? iris_buttons.skip_on_thumbs_down : true;

	    // Adjusts playlist for side-rail capabilities
	    var side_rail = options.settings.side_rail !== undefined ? options.settings.side_rail : false;
	    var related_rail = options.settings.related_rail !== undefined ? options.settings.related_rail : false;
	    var campaign_tracking = options.settings.campaign_tracking !== undefined ? options.settings.campaign_tracking : false;
	    var debug = options.debug ? options.debug : false;
	    var globalVar = options.global;

	    var validatedSettings = {
	      settings: {
	        number: number,
	        api_url: api_url,
	        base_url: base_url,
	        client_token: client_token,
	        platform: platform,
	        access_token: access_token,
	        ssl: ssl,
	        platform_id: platform_id,
	        player_version: player_version,
	        disable_mobile_upnext: disable_mobile_upnext,
	        up_next_min_vid_length: up_next_min_vid_length,
	        start_up_next: start_up_next,
	        start_up_next_text: start_up_next_text,
	        start_up_next_time: start_up_next_time,
	        start_up_next_length: start_up_next_length,
	        end_up_next: end_up_next,
	        end_up_next_text: end_up_next_text,
	        end_up_next_time: end_up_next_time,
	        end_up_next_length: end_up_next_length,
	        player_id: player_id,
	        campaign_tracking: campaign_tracking,
	        side_rail: side_rail,
	        related_rail: related_rail
	      },
	      player_elements: {
	        play_button: player_elements.play_button,
	        player_element: player_elements.player_element,
	        player_id: player_id,
	        video_player: video_player
	      },
	      iris_buttons: {
	        thumbs_up: thumbs_up,
	        thumbs_down: thumbs_down,
	        skip_forward: skip_forward,
	        skip_back: skip_back,
	        skip_on_thumbs_down: skip_on_thumbs_down
	      },
	      debug: debug,
	      global: globalVar
	    };

	    // Check for each Required Setting
	    requiredSettings.forEach(function (required_setting) {
	      if (!validatedSettings.settings[required_setting]) {
	        throw "Invalid Settings Error.  Missing or Invalid " + required_setting;
	      }
	    });

	    return validatedSettings;
	  }

	  var irisEngine = {
	    registerFunction: registerFunction,
	    registerEvent: registerEvent,
	    emit: emit,
	    forceWatchRequest: forceWatchRequest,
	    forceNextRequest: forceNextRequest,
	    addCallback: addCallback,
	    setPlaylist: setPlaylist,
	    getPlaylist: getPlaylist,
	    setAdState: setAdState,
	    getCurrentIndex: getCurrentIndex,
	    skipToIndex: skipToIndex
	  };

	  if (irisOptions.global) {
	    eval("window." + irisOptions.global + " = irisEngine");
	  }

	  return irisEngine;
	};

/***/ },
/* 1 */
/***/ function(module, exports, __webpack_require__) {

	'use strict';

	// This will hold the current state of the experience
	var AppDispatcher = __webpack_require__(2).Dispatcher;
	var Player = __webpack_require__(12);
	var Events = __webpack_require__(13);
	var Playlist = __webpack_require__(26);
	var Asset = __webpack_require__(27);
	var EventEmitter = __webpack_require__(10).EventEmitter;
	var assign = __webpack_require__(19);
	var EventConstants = __webpack_require__(20);
	var EnvironmentConstants = __webpack_require__(21);
	var Helpers = __webpack_require__(23);
	var IrisStartNextSlate = __webpack_require__(28);
	var IrisEndNextSlate = __webpack_require__(31);
	var Buttons = __webpack_require__(40);
	var logger = __webpack_require__(22);

	var CHANGE_EVENT = 'change';

	var _experience = {};

	var ExperienceStore = assign({}, EventEmitter.prototype, {
	  initialize: function initialize(eid, data) {
	    _experience[eid] = {
	      player: new Player(eid, data.player_elements),
	      currentAsset: function currentAsset() {
	        var asset = this.playlist.getInfo()[this.currentIndex];
	        if (asset === undefined) {
	          asset = new Asset({});
	        }
	        return asset;
	      },
	      playlist: new Playlist(eid),
	      behavior: {},
	      currentIndex: 0,
	      environment: data.settings,
	      ui_settings: data.iris_buttons,
	      id: eid
	    };

	    _experience[eid].environment.user_id = Helpers.setUserId();
	    _experience[eid].playlist.recs.push(new Asset({ platform_id: _experience[eid].environment.platform_id, eid: eid }));

	    // Check if next_slate is configured (default to false)
	    if (data.settings.start_up_next == true) {
	      var start_slate_settings = {
	        start_time: data.settings.start_up_next_time,
	        display_length: data.settings.start_up_next_length,
	        min_video_length: data.settings.up_next_min_vid_length,
	        up_next_text: data.settings.start_up_next_text
	      };
	      _experience[eid].irisStartNextSlate = new IrisStartNextSlate(eid, start_slate_settings);
	    }
	    if (data.settings.end_up_next == true) {
	      var end_slate_settings = {
	        start_time: data.settings.end_up_next_time,
	        display_length: data.settings.end_up_next_length,
	        min_video_length: data.settings.up_next_min_vid_length,
	        up_next_text: data.settings.end_up_next_text
	      };
	      _experience[eid].irisEndNextSlate = new IrisEndNextSlate(eid, end_slate_settings);
	    }
	    if (data.settings.side_rail == true) {
	      var SideRail = __webpack_require__(42);
	      _experience[eid].sideRail = new SideRail(eid, { "number": data.settings.number });
	    }
	    if (data.settings.related_rail == true) {
	      logger.debug("adding relatedRail");
	      var RelatedRail = __webpack_require__(43);
	      _experience[eid].relatedRail = new RelatedRail(eid);
	    }

	    Buttons.addIrisButtons(eid);

	    // Send new experience Event
	    logger.debug("IRIS: eid " + eid);
	    Events.new_experience(eid);

	    logger.debug("Experience: ", _experience);
	    return _experience[eid];
	  },
	  setInfo: function setInfo(id, infoObject) {
	    for (var key in infoObject) {
	      if (infoObject.hasOwnProperty(key)) {
	        _experience[id][key] = infoObject[key];
	      }
	    }
	    return _experience[id];
	  },
	  getInfo: function getInfo(id) {
	    return _experience[id];
	  },
	  getProtocol: function getProtocol(eid) {
	    var protocol = _experience[eid].environment.ssl == true ? 'https' : 'http';
	    return protocol;
	  },
	  getPlayer: function getPlayer(eid) {
	    return _experience[eid].player;
	  },
	  getCurrentAsset: function getCurrentAsset(id) {
	    var asset = _experience[id].currentAsset();
	    if (asset === undefined) {
	      asset = {};
	    }
	    return asset;
	  },
	  getCurrentIndex: function getCurrentIndex(eid) {
	    return _experience[eid].currentIndex;
	  },
	  getPlaylist: function getPlaylist(eid) {
	    return _experience[eid].playlist.recs;
	  },
	  registerPlaying: function registerPlaying(id) {
	    ExperienceStore.setInfo(id, { "playing": true });
	    ExperienceStore.getCurrentAsset(id).setInfo({
	      "playing": true
	    });
	    this.emitChange();
	  },
	  watch: function watch(id) {
	    Events.watch(id);
	  },
	  endOfPlaylist: function endOfPlaylist(id) {
	    return this.getCurrentIndex(id) >= this.getPlaylist(id).length - 1 ? true : false;
	  },
	  beginningOfPlaylist: function beginningOfPlaylist(eid) {
	    return ExperienceStore.getCurrentAsset(eid).platform_id == ExperienceStore.getPlaylist(eid)[0].platform_id;
	  },
	  addBehavior: function addBehavior(id, behaviorObject, addParams) {
	    for (var key in behaviorObject) {
	      if (behaviorObject.hasOwnProperty(key)) {
	        _experience[id]['behavior'][key] = behaviorObject[key];
	      }
	    }
	    Events.update(id, behaviorObject, addParams);
	    Events.addedBehavior(id, behaviorObject);
	    this.emitChange();
	  },
	  clearBehavior: function clearBehavior(behaviorObject) {
	    for (var key in behaviorObject) {
	      if (/^behavior*/.test(key)) {
	        delete _experience["behavior"][key.replace(/behavior\[/g, '').replace(/\]/g, '')];
	      }
	    }
	  },
	  setDefaultAsset: function setDefaultAsset(id) {
	    var defaultAssetInfo = {
	      "platform_id": _experience[id].environment.platform_id,
	      "unknown_asset": true
	    };
	    _experience[id].playlist.addToFirstPosition(defaultAssetInfo);
	  },
	  updateCurrentAsset: function updateCurrentAsset(id) {
	    var asset = _experience[id].currentAsset();
	    _experience[id].environment.platform_id = asset.platform_id;
	  },
	  getExperiences: function getExperiences() {
	    return _experience;
	  },
	  percentageWatched: function percentageWatched(id) {
	    return (_experience[id].player.currentTime(_experience[id].player.player_id) / _experience[id].player.currentDuration(_experience[id].player.player_id)).toFixed(2);
	  },
	  emitChange: function emitChange() {
	    this.emit(CHANGE_EVENT);
	  },
	  incrementCurrentIndex: function incrementCurrentIndex(id) {
	    _experience[id].currentIndex = _experience[id].currentIndex + 1;
	    Events.experienceUpdate(id);
	  },
	  decrementCurrentIndex: function decrementCurrentIndex(id) {
	    if (_experience[id].currentIndex > 0) {
	      _experience[id].currentIndex = _experience[id].currentIndex - 1;
	      Events.experienceUpdate(id);
	    }
	  },
	  skipToIndex: function skipToIndex(eid, index) {
	    _experience[eid].currentIndex = index;
	    Events.experienceUpdate(eid);
	  },
	  nextAsset: function nextAsset(id) {
	    var experience = _experience[id];
	    experience.successiveNextAttempts = experience.successiveNextAttempts ? experience.successiveNextAttempts : 0;
	    if (ExperienceStore.getCurrentIndex(id) >= ExperienceStore.getPlaylist(id).length - 1) {
	      experience.successiveNextAttempts++;
	      if (experience.successiveNextAttempts < 3) {
	        Events.next(id);
	      } else {
	        Events.emit(id, EventConstants.EXPERIENCE_ERROR);
	      }
	    } else {
	      experience.successiveNextAttempts = 0;
	      _experience[id].currentAsset().reset();
	      this.incrementCurrentIndex(id);
	    }
	  },
	  prevAsset: function prevAsset(id) {
	    _experience[id].currentAsset().reset();
	    this.decrementCurrentIndex(id);
	  },
	  displayStartSlate: function displayStartSlate(id) {
	    return _experience[id].environment.start_up_next;
	  },
	  displayEndSlate: function displayEndSlate(id) {
	    return _experience[id].environment.end_up_next;
	  }

	});

	AppDispatcher.register(function (action) {
	  switch (action.actionType) {
	    case EventConstants.NEW_EXPERIENCE:
	      ExperienceStore.emitChange();
	      break;

	    case EventConstants.FIRST_PLAY:
	      ExperienceStore.setInfo(action.experienceId, { "first_play": true });
	      if (ExperienceStore.getInfo(action.experienceId).watch_called !== true) {
	        ExperienceStore.watch(action.experienceId);
	      }
	      break;

	    case EventConstants.PLAYING:
	      var eid = action.experienceId;
	      if (ExperienceStore.getInfo(eid).watch_called !== true) {
	        ExperienceStore.watch(eid);
	      } else {
	        ExperienceStore.registerPlaying(eid, action.response);
	        ExperienceStore.addBehavior(eid, {
	          "play": 1
	        });
	      }

	      break;

	    case EventConstants.WATCH_START:
	      ExperienceStore.setInfo(action.experienceId, { "watch_called": true });
	      break;

	    case EventConstants.WATCH_SUCCESS:

	      ExperienceStore.setInfo(action.experienceId, { "experience": action.response.experience });
	      if (action.response.default_recs !== true) {
	        ExperienceStore.getInfo(action.experienceId).playlist.addToFirstPosition(action.response.next[0]);
	      }
	      ExperienceStore.getInfo(action.experienceId).playlist.addToPlaylist(action.response.next);
	      if (ExperienceStore.displayStartSlate(action.experienceId)) {
	        ExperienceStore.getInfo(action.experienceId).irisStartNextSlate.initialize();
	        ExperienceStore.getInfo(action.experienceId).irisStartNextSlate.populate();
	      }
	      if (ExperienceStore.displayEndSlate(action.experienceId)) {
	        ExperienceStore.getInfo(action.experienceId).irisEndNextSlate.initialize();
	        ExperienceStore.getInfo(action.experienceId).irisEndNextSlate.populate();
	      }
	      ExperienceStore.registerPlaying(action.experienceId);
	      break;

	    case EventConstants.WATCH_FAIL:
	      break;

	    case "PLAYLIST_CHANGE":
	      ExperienceStore.updateCurrentAsset(action.experienceId);
	      break;

	    case EventConstants.PW_02:
	      ExperienceStore.getInfo(action.experienceId).currentAsset().setInfo({
	        "PW_02": true
	      });
	      ExperienceStore.addBehavior(action.experienceId, {
	        "percentage_watched": action.data.percentageWatched
	      }, { "metrics_only": true });
	      break;

	    case EventConstants.PW_25:
	      ExperienceStore.getInfo(action.experienceId).currentAsset().setInfo({
	        "PW_25": true
	      });
	      ExperienceStore.addBehavior(action.experienceId, {
	        "percentage_watched": action.data.percentageWatched
	      }, { "metrics_only": true });
	      break;

	    case EventConstants.PW_50:
	      ExperienceStore.getInfo(action.experienceId).currentAsset().setInfo({
	        "PW_50": true
	      });
	      ExperienceStore.addBehavior(action.experienceId, {
	        "percentage_watched": action.data.percentageWatched
	      }, { "metrics_only": true });
	      break;

	    case EventConstants.PW_75:
	      ExperienceStore.getInfo(action.experienceId).currentAsset().setInfo({
	        "PW_75": true
	      });
	      ExperienceStore.addBehavior(action.experienceId, {
	        "percentage_watched": action.data.percentageWatched
	      }, { "metrics_only": true });
	      break;
	    case EventConstants.UPDATE_START:
	      break;
	    case EventConstants.UPDATE_SUCCESS:
	      if (action.response.behavior.thumbs_up == 1 || action.response.behavior.thumbs_down == 1) {
	        Events.next(action.experienceId);
	      }
	      break;

	    case EventConstants.NEXT_SUCCESS:
	      ExperienceStore.getInfo(action.experienceId).playlist.addToPlaylist(action.response.next);
	      break;
	    case EventConstants.ENDED:
	      var asset = ExperienceStore.getCurrentAsset(action.experienceId);
	      var percentageWatched = ExperienceStore.percentageWatched(action.experienceId);
	      var behavior;

	      asset.setInfo({ "ended": true });
	      if (action.data !== undefined) {
	        behavior = {
	          "percentage_watched": action.data.percentageWatched
	        };
	        if (action.data.type == "next_auto") {
	          behavior.next_auto = 1;
	        }
	      } else {
	        behavior = {
	          "percentage_watched": percentageWatched
	        };
	      }

	      if (percentageWatched >= 0.98 || parseInt(action.data.percentageWatched) >= 0.98) {
	        behavior.video_complete = 1;
	      }
	      ExperienceStore.addBehavior(action.experienceId, behavior);

	      logger.debug("IRIS: NEXT ASSET");
	      ExperienceStore.nextAsset(action.experienceId);

	      break;

	    case EventConstants.EXPERIENCE_UPDATE:

	      if (ExperienceStore.endOfPlaylist(action.experienceId)) {
	        logger.debug("IRIS: end of playlist");
	        Events.next(action.experienceId);
	      }
	      ExperienceStore.updateCurrentAsset(action.experienceId);
	      if (ExperienceStore.getInfo(action.experienceId).currentAsset().played !== true) {
	        ExperienceStore.getInfo(action.experienceId).player.play(ExperienceStore.getInfo(action.experienceId).currentAsset(), ExperienceStore.getInfo(action.experienceId).environment.player_id);
	      }
	      break;
	    case EventConstants.NEW_ASSET:
	      ExperienceStore.play(action.experienceId);
	      break;
	    case EventConstants.THUMBS_DOWN:
	      if (!(ExperienceStore.getInfo(action.experienceId).currentAsset().thumbs_down == 1) && !(ExperienceStore.getInfo(action.experienceId).currentAsset().thumbs_up == 1)) {
	        ExperienceStore.getInfo(action.experienceId).currentAsset().setInfo({ "thumbs_down": 1 });
	        ExperienceStore.addBehavior(action.experienceId, {
	          "thumbs_down": 1,
	          "percentage_watched": ExperienceStore.percentageWatched(action.experienceId)
	        });
	        logger.debug("SKIP ON THUMBS_DOWN? " + ExperienceStore.getInfo(action.experienceId).ui_settings.skip_on_thumbs_down);
	        if (ExperienceStore.getInfo(action.experienceId).ui_settings.skip_on_thumbs_down === true) {
	          ExperienceStore.nextAsset(action.experienceId);
	        }
	      }
	      break;
	    case EventConstants.THUMBS_UP:
	      if (!(ExperienceStore.getInfo(action.experienceId).currentAsset().thumbs_up == 1) && !(ExperienceStore.getInfo(action.experienceId).currentAsset().thumbs_down == 1)) {
	        ExperienceStore.getInfo(action.experienceId).currentAsset().setInfo({ "thumbs_up": 1 });
	        ExperienceStore.addBehavior(action.experienceId, {
	          "thumbs_up": 1,
	          "percentage_watched": ExperienceStore.percentageWatched(action.experienceId)
	        });
	      }
	      break;
	    case EventConstants.SKIP_FORWARD:
	      ExperienceStore.addBehavior(action.experienceId, {
	        "next": 1,
	        "percentage_watched": ExperienceStore.percentageWatched(action.experienceId)
	      });
	      ExperienceStore.nextAsset(action.experienceId);
	      break;
	    case EventConstants.SKIP_BACK:
	      ExperienceStore.addBehavior(action.experienceId, {
	        "next": 1,
	        "percentage_watched": ExperienceStore.percentageWatched(action.experienceId)
	      });
	      ExperienceStore.prevAsset(action.experienceId);
	      break;
	    case EventConstants.PLAY:
	      ExperienceStore.setInfo(action.experienceId, { "pauseBeacons": false });
	      break;
	    default:
	      break;
	  }
	});

	module.exports = ExperienceStore;

