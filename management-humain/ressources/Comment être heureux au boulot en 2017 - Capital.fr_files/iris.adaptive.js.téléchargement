	        } else {
	            document.getElementById(end_slate.settings.slate_image_id).style.backgroundImage = "url(" + asset.image_url + ")";
	        }
	        return true;
	    } else {
	        return false;
	    }
	};

	module.exports = IrisEndNextSlate;

