/***/ function(module, exports, __webpack_require__) {

	'use strict';

	var Events = __webpack_require__(13);
	var Helpers = __webpack_require__(23);
	var jQuery = __webpack_require__(24);
	var logger = __webpack_require__(22);
	var EventConstants = __webpack_require__(20);
	var AppDispatcher = __webpack_require__(2).Dispatcher;
	var dispatch = __webpack_require__(2).dispatch;
	var Api = __webpack_require__(14);
	var Experience;

	// TODO: 
	//  - make HTML IDs for previous assets in playlist [-1, -2, -3,...]
	//  - Next call everytime? 
	//    - pull `number` everytime, if playlist is < 10, make another next call
	//    - for iOS, mark currentIndex+`number` as safe and chop off after that

	var _relatedRail = false;
	// Constructor
	var RelatedRail = function RelatedRail(eid, options) {
	  _relatedRail = true;
	  Experience = __webpack_require__(1);
	  // attach eid.
	  this.eid = eid;
	  this.recs = [];
	  // attach options passed in by plugin. 
	  for (var key in options) {
	    if (options.hasOwnProperty(key)) {
	      this[key] = options[key];
	    }
	  }
	  this.addEventListeners(eid);
	  logger.debug(this);
	  return this;
	};

	// CHANGE THIS: 
	//  over-ride Playlist.addToPlaylist function to also add to the recs list 
	RelatedRail.prototype.updateRecsList = function () {
	  var currentPlaylist = Experience.getPlaylist(this.eid);
	  this.recs = currentPlaylist;
	  return this;
	};

	RelatedRail.prototype.getInfo = function () {
	  return this;
	};

	RelatedRail.prototype.updateHTMLRail = function () {
	  var railDivs = jQuery("[data-iris-related]");
	  var asset;
	  for (var i = 0; i <= railDivs.length - 1; i++) {
	    var playlistIndex = Experience.getCurrentIndex(this.eid) + parseInt(railDivs[i].getAttribute("data-iris-related"));
	    // get asset at playlistIndex
	    asset = this.recs[playlistIndex];
	    if (asset === undefined) {
	      resetDiv(railDivs[i]);
	    }
	    // update innerHTML of .title, .image_url, .description with asset's values
	    if (asset.valid) {
	      if (railDivs[i].getElementsByClassName("title").length > 0) {
	        railDivs[i].getElementsByClassName("title")[0].innerHTML = asset.title;
	      }
	      if (railDivs[i].getElementsByClassName("thumbnail").length > 0) {
	        railDivs[i].getElementsByClassName("thumbnail")[0].innerHTML = "<img src='" + asset.image_url + "'/>";
	      }
	    }
	  }
	};
	// Helper. Refactor.
	function resetDiv(div) {
	  // reset the div to show nothing. What do we want to do here? 
	  div.getElementsByClassName("title")[0].innerHTML = '';
	  div.getElementsByClassName("thumbnail")[0].innerHTML = '';
	}

	RelatedRail.prototype.addEventListeners = function () {
	  var railDivs = jQuery("[data-iris-related]");
	  var relatedRail = this;
	  for (var i = 0; i <= railDivs.length - 1; i++) {
	    railDivs[i].addEventListener('click', function () {
	      var assetIndex = parseInt(this.getAttribute("data-iris-related"));
	      var asset = relatedRail.recs[Experience.getCurrentIndex(relatedRail.eid) + assetIndex];
	      Experience.getInfo(relatedRail.eid).playlist.addToNextPosition(relatedRail.eid, asset);
	      Events.skip_forward(relatedRail.eid);
	    });
	  }
	};

	AppDispatcher.register(function (action) {
	  switch (action.actionType) {
	    case EventConstants.WATCH_SUCCESS:
	      if (_relatedRail) {
	        // Experience.getInfo(action.experienceId).relatedRail.updateHTMLRail();          
	      }
	      break;
	    case EventConstants.PLAYLIST_CHANGE:
	      if (_relatedRail) {
	        Experience.getInfo(action.experienceId).relatedRail.updateRecsList();
	        Experience.getInfo(action.experienceId).relatedRail.updateHTMLRail();
	      }
	      break;
	    case EventConstants.UPDATE_SUCCESS:
	      if (_relatedRail) {
	        Experience.getInfo(action.experienceId).relatedRail.updateHTMLRail();
	      }
	      break;
	    case EventConstants.ENDED:
	      if (_relatedRail) {
	        Api.make_next_request(action.experienceId);
	      }
	      break;
	    case EventConstants.SKIP_FORWARD:
	      if (_relatedRail) {
	        Api.make_next_request(action.experienceId);
	      }
	      break;

	  };
	});

	module.exports = RelatedRail;

