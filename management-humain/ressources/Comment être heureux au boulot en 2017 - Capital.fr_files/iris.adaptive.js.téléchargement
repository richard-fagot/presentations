/* 42 */
/***/ function(module, exports, __webpack_require__) {

	'use strict';

	var Events = __webpack_require__(13);
	var Helpers = __webpack_require__(23);
	var jQuery = __webpack_require__(24);
	var logger = __webpack_require__(22);
	var EventConstants = __webpack_require__(20);
	var AppDispatcher = __webpack_require__(2).Dispatcher;
	var dispatch = __webpack_require__(2).dispatch;
	var Api = __webpack_require__(14);
	var Experience;

	// TODO: 
	//  - make HTML IDs for previous assets in playlist [-1, -2, -3,...]
	//  - Next call everytime? 
	//    - pull `number` everytime, if playlist is < 10, make another next call
	//    - for iOS, mark currentIndex+`number` as safe and chop off after that

	var _sideRail = false;
	// Constructor
	var SideRail = function SideRail(eid, options) {
	  _sideRail = true;
	  Experience = __webpack_require__(1);
	  // attach eid.
	  this.eid = eid;
	  // attach options passed in by plugin. 
	  for (var key in options) {
	    if (options.hasOwnProperty(key)) {
	      this[key] = options[key];
	    }
	  }
	  // console.log(eid);
	  this.addEventListeners(eid);
	  return this;
	};

	SideRail.prototype.getInfo = function () {
	  return this;
	};

	SideRail.prototype.updateHTMLRail = function () {
	  // console.log("updating left-rail")
	  var railDivs = jQuery("[data-iris-rail]");
	  var asset;
	  for (var i = 0; i <= railDivs.length - 1; i++) {
	    var playlistIndex = Experience.getCurrentIndex(this.eid) + parseInt(railDivs[i].getAttribute("data-iris-rail"));
	    // get asset at playlistIndex
	    asset = Experience.getPlaylist(this.eid)[playlistIndex];
	    if (asset === undefined) {
	      return;
	    }
	    // update innerHTML of .title, .image_url, .description with asset's values
	    if (asset.valid) {
	      if (railDivs[i].getElementsByClassName("title").length > 0) {
	        railDivs[i].getElementsByClassName("title")[0].innerHTML = asset.title;
	      }
	      if (railDivs[i].getElementsByClassName("thumbnail").length > 0) {
	        railDivs[i].getElementsByClassName("thumbnail")[0].innerHTML = "<img src='" + asset.image_url + "'/>";
	      }
	    }
	    // console.log(asset);
	    this.setAssetAsShown(asset);
	  }
	};

	SideRail.prototype.addEventListeners = function () {
	  var railDivs = jQuery("[data-iris-rail]");
	  var sideRail = this;
	  for (var i = 0; i <= railDivs.length - 1; i++) {
	    railDivs[i].addEventListener('click', function () {
	      // console.log(this.getAttribute("data-iris-rail"));
	      Experience.skipToIndex(sideRail.eid, Experience.getCurrentIndex(sideRail.eid) + parseInt(this.getAttribute("data-iris-rail")));
	    });
	  }
	};

	SideRail.prototype.setAssetAsShown = function (asset) {
	  // console.log("ASSET", asset)
	  asset.setInfo({ "sideRailShown": true });
	};

	AppDispatcher.register(function (action) {
	  switch (action.actionType) {
	    case EventConstants.WATCH_SUCCESS:
	      if (_sideRail) {
	        Experience.getInfo(action.experienceId).sideRail.updateHTMLRail();
	      }
	      break;
	    case EventConstants.PLAYLIST_CHANGE:
	      if (_sideRail) {
	        Experience.getInfo(action.experienceId).sideRail.updateHTMLRail();
	      }
	      break;
	    case EventConstants.UPDATE_SUCCESS:
	      if (_sideRail) {
	        Experience.getInfo(action.experienceId).sideRail.updateHTMLRail();
	      }
	      break;
	    case EventConstants.ENDED:
	      if (_sideRail) {
	        Api.make_next_request(action.experienceId);
	      }
	      break;
	    case EventConstants.SKIP_FORWARD:
	      if (_sideRail) {
	        Api.make_next_request(action.experienceId);
	      }
	      break;
	    case EventConstants.EXPERIENCE_UPDATE:
	      if (_sideRail) {
	        if (Experience.getPlaylist(action.experienceId).slice(Experience.getCurrentIndex(action.experienceId), Experience.getPlaylist(action.experienceId).length).length < Experience.getInfo(action.experienceId).environment.number) {
	          Api.make_next_request(action.experienceId);
	        }
	      }
	      break;
	  };
	});

	module.exports = SideRail;

/***/ },
