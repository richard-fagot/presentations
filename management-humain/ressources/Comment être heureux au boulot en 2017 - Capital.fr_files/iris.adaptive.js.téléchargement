	        key: 'dispatch',
	        value: function dispatch(payload, cb) {
	            invariant(!this._isDispatching, 'Dispatch.dispatch(...): Cannot dispatch in the middle of a dispatch.');
	            this._startDispatching(payload);
	            try {
	                for (var id in this._callbacks) {
	                    if (this._isPending[id]) {
	                        continue;
	                    }
	                    this._invokeCallback(id);
	                }
	            } finally {
	                this._stopDispatching();
	                cb();
	            }
	        }
	    }, {
	        key: 'onQueueEnd',
	        value: function onQueueEnd(cb) {
	            this._actionQueue.on('end', cb.bind(this));
	        }
	    }, {
	        key: 'queueDispatch',
	        value: function queueDispatch(payload) {
	            this._actionQueue.push(function (cb) {
	                this.dispatch(payload, cb);
	            }.bind(this));

	            if (!this._actionQueueActive) {
	                this._actionQueueActive = true;
	                this._actionQueue.start();
	            }
	        }
	    }]);

