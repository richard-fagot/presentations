	        var _this = _possibleConstructorReturn(this, (QDispatcher.__proto__ || Object.getPrototypeOf(QDispatcher)).call(this));

	        _this._actionQueue = new Queue();
	        _this._actionQueue.concurrency = 1; // make it synchronous
	        _this._actionQueueActive = false;
	        _this._actionQueue.on('end', function () {
	            this._actionQueueActive = false;
	        }.bind(_this));
	        return _this;
