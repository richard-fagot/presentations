/***/ function(module, exports, __webpack_require__) {

	'use strict';

	var Events = __webpack_require__(13);
	var Helpers = __webpack_require__(23);
	var jQuery = __webpack_require__(24);
	var logger = __webpack_require__(22);
	var AppDispatcher = __webpack_require__(2).Dispatcher;
	var EventConstants = __webpack_require__(20);
	var Experience;
	var Player = function Player(eid, options) {
	  Experience = __webpack_require__(1);
	  // Give player a reference to the experience ID
	  this.eid = eid;
	  this.ads = {};
	  this.ads.state = false;
	  for (var key in options) {
	    if (options.hasOwnProperty(key)) {
	      this[key] = options[key];
	    }
	  }
	  // get default player element
	  if (this.player_element === undefined) {
	    this.player_element = document.getElementById(this.player_id);
	    this.video_player = this.player_element;
	  }

	  return this;
	};
	Player.prototype.currentTime = function () {
	  logger.warn("Please register currentTime function");
