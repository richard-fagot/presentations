(function(){var script=document.createElement("script");script.src="https://t.infoprodata.com/divolte.js";script.type="text/javascript";script.async="true";script.defer="true";script.onload=function(){if(divolte!="undefined"){setGlobalId();}script.onload=null;script.onreadystatechange=null;};script.onreadystatechange=function(){if(script.readyState=="complete"){if(divolte!="undefined"){setGlobalId();}script.onload=null;script.onreadystatechange=null;}else{if(script.readyState=="loaded"){eval(script);if(divolte!="undefined"){setGlobalId();}script.onload=null;script.onreadystatechange=null;}}};var script0=document.getElementsByTagName("script")[0];script0.parentNode.insertBefore(script,script0);$("body").on("click","input[type='submit']",function(){var email=$("input[name^='email']").val();if(email!=""&&email!=null){divolte.signal("pageParams",{ue:email,xtpage:"",tags:"",site:site});}});})();