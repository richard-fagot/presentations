(function($){var q=0;$(window).data("ajaxready",true);$.fn.infiniteScroll=function(options,callback){var opts=$.extend({},$.fn.infiniteScroll.defaults,options);var stop=false;$(window).scroll(function(){var pixelsFromWindowBottomToBottom=0+$(document).height()-($(document).scrollTop())-$(window).height();if(pixelsFromWindowBottomToBottom<100&&$(window).data("ajaxready")){$(window).data("ajaxready",false);if(stop){return;}$.ajax({url:opts.url+"&q="+q,data:opts.data}).success(function(data){if(data.response.content){jQuery(opts.container).before(data.response.content);}else{stop=true;}$(window).data("ajaxready",true);q++;if(typeof callback=="function"){callback.call(this,data);}});}});return this;};$.fn.infiniteScroll.defaults={url:"/ajax.php?method=getHomeThematique",container:".footer",data:{}};}(jQuery));